<?xml version="1.0" encoding="utf-8"?>
<!--选中红点图标-->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/sp_ic_checked_disable" android:state_enabled="false" />
    <item android:drawable="@drawable/sp_ic_checked" android:state_checked="true" />
    <item android:drawable="@drawable/sp_ic_checked" android:state_pressed="true" />
    <item android:drawable="@drawable/sp_ic_checked" android:state_activated="true" />
    <item android:drawable="@drawable/sp_ic_unchecked" />
</selector>