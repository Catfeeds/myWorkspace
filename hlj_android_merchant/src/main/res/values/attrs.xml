<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TextView">
        <attr name="textStyle">
            <flag name="normal" value="0"/>
            <flag name="bold" value="1"/>
            <flag name="italic" value="2"/>
            <flag name="black" value="8"/>
            <flag name="condensed" value="16"/>
            <flag name="light" value="32"/>
            <flag name="medium" value="64"/>
            <flag name="thin" value="128"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="ChartView">
        <attr name="gridLineColor" format="reference|color"/>
        <attr name="shadowColor" format="reference|color"/>
        <attr name="gridLineWidth" format="reference|dimension"/>
        <attr name="gridLinesHorizontal" format="reference|integer"/>
        <attr name="gridLinesVertical" format="reference|integer"/>
        <attr name="leftLabelWidth" format="reference|dimension"/>
        <attr name="topLabelHeight" format="reference|dimension"/>
        <attr name="rightLabelWidth" format="reference|dimension"/>
        <attr name="bottomLabelHeight" format="reference|dimension"/>
    </declare-styleable>

    <declare-styleable name="MessageImageView">
        <attr name="mask" format="reference"/>
    </declare-styleable>

    <declare-styleable name="CheckableLinearLayout2">
        <attr name="isChecked" format="boolean" />
        <attr name="uncheckable" format="boolean" />
    </declare-styleable>


    <declare-styleable name="CheckableRelativeLayout">
        <attr name="isChecked2" format="boolean" />
        <attr name="uncheckable2" format="boolean" />
    </declare-styleable>
    <declare-styleable name="ShSwitchView">
        <attr name="tintColor" format="reference|color" />
        <attr name="clickCheckable" format="boolean" />
        <attr name="outerStrokeWidth" format="reference|dimension" />
        <attr name="shadowSpace" format="reference|dimension" />
    </declare-styleable>
    <declare-styleable name="FlowLayout">
        <attr name="horizontalSpacing" format="dimension" />
        <attr name="verticalSpacing" format="dimension" />
    </declare-styleable>
    <declare-styleable name="FlowLayout_LayoutParams">
        <attr name="layout_breakLine" format="boolean" />
        <attr name="layout_horizontalSpacing" format="dimension" />
    </declare-styleable>
    <declare-styleable name="RatingView">
        <attr name="offDrawable" format="reference" />
        <attr name="onDrawable" format="reference" />
        <attr name="itemMargin" format="dimension" />
        <attr name="itemHeight" format="dimension" />
        <attr name="isindicator" format="boolean" />
        <attr name="isHalf" format="boolean" />
        <attr name="halfDrawable" format="reference"/>
    </declare-styleable>
</resources>