<manifest package="com.hunliji.hljcardcustomerlibrary"

    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity
            android:name=".views.activities.CardListActivity"
            android:label="@string/title_activity_card_list___card"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/CustomTitleTheme.Dark" />

        <activity
            android:name=".views.activities.CardInfoEditActivity"
            android:label="@string/title_activity_card_info___card"
            android:screenOrientation="portrait"
            android:theme="@style/CustomTitleTheme.Dark"
            android:windowSoftInputMode="stateAlwaysHidden|adjustResize" />
        <activity
            android:name=".views.activities.CardTutorialActivity"
            android:label="@string/title_activity_card_tutorial___card"
            android:screenOrientation="portrait"
            android:theme="@style/CustomTitleTheme.Dark"
            android:windowSoftInputMode="stateHidden|adjustPan" />

        <activity
            android:name=".views.activities.CardReplyActivity"
            android:label="@string/title_activity_card_reply___card"
            android:screenOrientation="portrait"
            android:theme="@style/CustomTitleTheme.Dark" />
        <activity
            android:name=".views.activities.CardSettingActivity"
            android:label="@string/title_activity_card_setting___card"
            android:screenOrientation="portrait"
            android:theme="@style/CustomTitleTheme.Dark" />
        <activity
            android:name=".views.activities.BindWXSettingActivity"
            android:label="@string/title_withdraw_setting___card"
            android:screenOrientation="portrait"
            android:theme="@style/CustomTitleTheme.Dark" />
        <activity
            android:name=".views.activities.BindBankSettingActivity"
            android:label="@string/title_withdraw_setting___card"
            android:screenOrientation="portrait"
            android:theme="@style/CustomTitleTheme.Dark"
            android:windowSoftInputMode="stateHidden|adjustResize" />
        <activity
            android:name=".views.activities.ReceiveGiftCashActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:theme="@style/CustomTitleTheme.Dark"
            android:windowSoftInputMode="stateHidden|adjustPan" />
        <activity
            android:name=".views.activities.CardSendActivity"
            android:label="发送"
            android:screenOrientation="portrait"
            android:theme="@style/CustomTitleTheme"
            android:windowSoftInputMode="stateHidden|adjustPan" />
        <activity
            android:name=".views.activities.ReplyCardUserActivity"
            android:theme="@style/DialogActivityTheme"
            android:windowSoftInputMode="stateVisible|adjustResize" />

        <activity
            android:name=".views.activities.BalanceActivity"
            android:label="@string/label_balance"
            android:screenOrientation="portrait"
            android:theme="@style/CustomTitleTheme.Dark" />
        <activity
            android:name=".views.activities.AfterWithdrawCashActivity"
            android:label="@string/label_withdraw_cash_success_title"
            android:screenOrientation="portrait"
            android:theme="@style/CustomTitleTheme.Dark"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".views.activities.WithdrawActivity"
            android:label="@string/label_withdraw_cash_title"
            android:screenOrientation="portrait"
            android:theme="@style/CustomTitleTheme.Dark"
            android:windowSoftInputMode="adjustResize|stateHidden" />

        <activity
            android:name=".views.activities.WithdrawCardListActivity"
            android:label="@string/label_withdraw_cash_title"
            android:screenOrientation="portrait"
            android:theme="@style/CustomTitleTheme.Dark" />
        <activity
            android:name=".views.activities.WithdrawV2Activity"
            android:label="@string/label_withdraw_cash_confirm"
            android:screenOrientation="portrait"
            android:theme="@style/CustomTitleTheme.Dark" />
        <activity
            android:name=".views.activities.FundManageActivity"
            android:screenOrientation="portrait"
            android:theme="@style/CustomTitleTheme" />
        <activity
            android:name=".views.activities.BindFundBankActivity"
            android:label="@string/title_activity_fund_bind_bank"
            android:screenOrientation="portrait"
            android:theme="@style/CustomTitleTheme" />
        <activity
            android:name=".views.activities.FundDetailActivity"
            android:label="@string/label_fund_detail"
            android:screenOrientation="portrait"
            android:theme="@style/CustomTitleTheme" />
        <activity
            android:name=".views.activities.FundSettingActivity"
            android:label="@string/label_fund_setting"
            android:screenOrientation="portrait"
            android:theme="@style/CustomTitleTheme" />
        <activity
            android:name=".views.activities.FundMyBankActivity"
            android:label="@string/title_activity_my_bank"
            android:screenOrientation="portrait"
            android:theme="@style/CustomTitleTheme" />
        <activity
            android:name=".views.activities.AfterRollInOutActivity"
            android:label="@string/title_activity_after_roll_in"
            android:screenOrientation="portrait"
            android:theme="@style/CustomTitleTheme" />
        <activity
            android:name=".views.activities.CardRollInFundActivity"
            android:label="@string/title_activity_card_roll_in"
            android:screenOrientation="portrait"
            android:theme="@style/CustomTitleTheme" />
        <activity
            android:name=".views.activities.BankRollInFundActivity"
            android:label="@string/label_roll_in"
            android:screenOrientation="portrait"
            android:theme="@style/CustomTitleTheme" />
        <activity
            android:name=".views.activities.BankRollOutFundActivity"
            android:label="@string/label_roll_out_fund"
            android:screenOrientation="portrait"
            android:theme="@style/CustomTitleTheme" />
        <activity
            android:name=".views.activities.AccountSecurityActivity"
            android:label="@string/title_activity_account_security"
            android:screenOrientation="portrait"
            android:theme="@style/CustomTitleTheme" />
        <activity
            android:name=".views.activities.BindUserPhoneActivity"
            android:label="@string/title_activity_bind_card_phone"
            android:screenOrientation="portrait"
            android:theme="@style/CustomTitleTheme" />
        <activity
            android:name=".views.activities.ChangeBindPhoneActivity"
            android:label="@string/title_activity_change_bind_phone"
            android:screenOrientation="portrait"
            android:theme="@style/CustomTitleTheme" />
        <activity android:name=".views.activities.EditRealNameActivity"
            android:label="@string/title_activity_edit_real_name"
            android:screenOrientation="portrait"
            android:theme="@style/CustomTitleTheme"/>
    </application>

</manifest>
