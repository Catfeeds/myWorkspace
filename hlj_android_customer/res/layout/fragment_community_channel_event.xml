<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.hunliji.hljcommonlibrary.views.widgets.HljNestedScrollView
        android:layout_width="match_parent"
        android:id="@+id/scrollView"
        android:overScrollMode="never"
        android:layout_height="match_parent">

        <LinearLayout
            android:id="@+id/ll_web_view"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <com.example.suncloud.hljweblibrary.views.widgets.CustomWebView
                android:id="@+id/web_view"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <View
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:background="@color/colorWhite" />
        </LinearLayout>
    </com.hunliji.hljcommonlibrary.views.widgets.HljNestedScrollView>
    <com.hunliji.hljcommonlibrary.views.widgets.HljEmptyView
        android:id="@+id/empty_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
</RelativeLayout>