<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="@color/colorWhite" />
        </shape>
    </item>
    <item
        android:left="50dp"
        android:right="-1dp"
        android:top="-1dp">
        <shape>
            <solid android:color="@color/colorWhite"/>
            <stroke
                android:width="0.8dp"
                android:color="@color/colorLine" />
        </shape>
    </item>

    <item
        android:bottom="0.8dp"
        android:top="0dp"
        android:left="0dp"
        android:right="0dp">
        <shape>
            <solid android:color="@color/colorWhite" />
        </shape>
    </item>

</layer-list>